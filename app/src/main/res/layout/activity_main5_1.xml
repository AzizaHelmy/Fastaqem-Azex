<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main3_1"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/sss"
    tools:context="com.example.doaa.fastaqem.Main3_1Activity">
    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:hint="أدخل ماتريد تذكره:"
        android:layout_gravity="center_horizontal"
        android:textColorHint="#26596b"
        android:textColor="#26596b"
        android:id="@+id/e5_1"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="37dp"
        android:layout_marginBottom="34dp"
        android:layout_gravity="center_horizontal"
        android:background="@drawable/buttonshapeadd"
        android:textSize="20sp"
        android:onClick="backToAdd"
        android:textColor="#2c4f5a"
        android:text="إضافة"/>
</LinearLayout>
